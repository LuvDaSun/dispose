{
  "name": "using-disposable",
  "version": "1.0.0",
  "description": "",
  "main": "bin/index.js",
  "types": "bin/index.d.ts",
  "scripts": {
    "test": "npm run lint & npm run spec-all",
    "compile": "tsc",
    "lint": "tslint 'src/**.ts'",
    "spec": "tape --require 'ts-node/register'",
    "spec-all": "npm run spec 'src/**/*.spec.ts'",
    "coverage": "nyc --reporter text-summary --reporter lcov --include 'src/**/*.ts' --exclude 'src/**/*.spec.ts' --extension '.ts' npm run spec-all",
    "prepublish": "npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LuvDaSun/using-disposable.git"
  },
