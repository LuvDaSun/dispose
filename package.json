  "devDependencies": {
    "@types/blue-tape": "^0.1.31",
    "@types/node": "^9.4.0",
    "@types/sinon": "^4.1.3",
    "@types/tape": "^4.2.31",
    "blue-tape": "^1.0.0",
    "nyc": "^11.4.1",
    "sinon": "^4.2.2",
    "tape": "^4.8.0",
    "ts-node": "^4.1.0",
    "tslint": "^5.9.1",
