  "dependencies": {
    "tslib": "^1.7.1"
  },
  "devDependencies": {
    "@types/node": "^6.0.45",
    "@types/sinon": "^2.2.2",
    "@types/tape": "^4.2.30",
    "nyc": "^10.3.2",
    "sinon": "^2.2.0",
    "tape": "^4.6.3",
    "ts-node": "^3.0.4",
    "tslint": "^5.2.0",
    "typescript": "^2.3.2"
  }
