  },
  "homepage": "https://github.com/LuvDaSun/using-disposable#readme",
  "dependencies": {
    "tslib": "^1.9.0"
  },
  "devDependencies": {
    "@types/blue-tape": "^0.1.31",
    "@types/node": "^9.3.0",
    "@types/sinon": "^4.1.3",
    "@types/tape": "^4.2.31",
    "blue-tape": "^1.0.0",
    "nyc": "^11.4.1",
    "rollup": "^0.54.1",
    "sinon": "^4.1.6",
    "tape": "^4.8.0",
